<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>The Grand List</title>
  </head>
  <body>
    <h1>Places I've Been</h1>
      <ul>
        @foreach (var place in Model)
        {
          <li><a href="/places/@place.GetCityName()">@place.GetCityName()</a></li>
        }
      </ul>
      <a href="/add_place">Add a New Place</a>
      <a href="/">Home</a>
  </body>
</html>
